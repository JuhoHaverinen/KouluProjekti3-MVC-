@model IEnumerable<FFMP.Data.AuditingForm>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Created)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserLoginNavigation)
            </th>
            <th>
                Target group
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Created)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserLoginNavigation.Login)
            </td>
            <td>
                    @Html.DisplayFor(modelItem => item.TargetGroup.Description)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AuditingId">Edit</a> |
                <a asp-action="Delete" asp-route-id="@item.AuditingId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
